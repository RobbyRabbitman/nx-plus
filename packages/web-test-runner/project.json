{
  "name": "web-test-runner",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "tags": ["scope:node", "type:lib"],
  "targets": {
    "build": {
      "options": {
        "assets": [
          "{projectRoot}/*.md",
          {
            "input": "{projectRoot}/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "{projectRoot}",
            "glob": "generators.json",
            "output": "."
          }
        ]
      }
    }
  }
}
