{
  "name": "@robby-rabbitman/nx-plus-web-dev-server",
  "version": "0.6.2",
  "description": "Web Dev Server plugin for Nx workspaces.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobbyRabbitman/nx-plus.git",
    "directory": "packages/web-dev-server"
  },
  "keywords": [
    "Nx",
    "Web Dev Server",
    "Plugin",
    "Monorepo",
    "Nrwl",
    "Modern Web",
    "Generator",
    "Web Development",
    "Web"
  ],
  "author": "Robby Rabbitman",
  "homepage": "https://github.com/RobbyRabbitman/nx-plus/tree/main/packages/web-dev-server",
  "bugs": {
    "url": "https://github.com/RobbyRabbitman/nx-plus/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nx/devkit": "^20.0.0"
  },
  "devDependencies": {
    "vitest": "1.6.0",
    "memfs": "4.14.0",
    "minimatch": "10.0.1",
    "@robby-rabbitman/nx-plus-tools-vite": "workspace:*",
    "@robby-rabbitman/nx-plus-tools-eslint": "workspace:*"
  },
  "files": [
    "dist/src",
    "dist/cjs",
    "generators.json",
    "package.json"
  ],
  "generators": "./generators.json",
  "type": "module",
  "main": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/src/index.d.ts",
      "default": "./dist/esm/src/index.js"
    },
    "./plugins/web-dev-server": {
      "import": "./dist/esm/src/plugins/web-dev-server.plugin.js",
      "require": "./dist/cjs/src/plugins/web-dev-server.plugin.js",
      "types": "./dist/esm/src/plugins/web-dev-server.plugin.d.ts",
      "default": "./dist/esm/src/plugins/web-dev-server.plugin.js"
    }
  }
}
