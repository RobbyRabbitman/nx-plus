{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "dist/spec",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo/tsconfig.spec.tsbuildinfo",
    "types": [
      "vitest/globals",
      "vitest/importMeta",
      "vite/client",
      "node",
      "vitest"
    ]
  },
  "include": [
    "vite.config.ts",
    "vitest.config.mts",
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}
