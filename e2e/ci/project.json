{
  "name": "e2e-ci",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "tags": ["scope:node", "type:util"],
  "targets": {
    "build": { "executor": "nx:noop", "inputs": [], "cache": true },
    "nx-release-publish": {
      "executor": "nx:noop"
    },
    "e2e-vite-version-matrix": {
      "cache": true,
      "inputs": ["publishable"],
      "command": "ts-node {projectRoot}/src/e2e-vite-version-matrix.ts",
      "options": {
        "project": "{projectRoot}/tsconfig.lib.json"
      }
    }
  }
}
