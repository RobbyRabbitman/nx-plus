    "minimatch": "10.0.1",
    "mocha": "11.1.0",
    "nx": "20.6.4",
    "prettier": "3.5.3",
    "prettier-plugin-jsdoc": "1.3.2",
    "prettier-plugin-sh": "0.17.1",
    "renovate": "39.233.3",
