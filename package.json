{
  "name": "@robby-rabbitman/nx-plus",
  "version": "0.7.0",
  "scripts": {
    "clean": "git clean -dfX && pnpm i && pnpm nx reset && pnpm nx sync",
    "postinstall": "sh tools/workspace/src/npm-life-cycle/post-install.sh",
    "sync-dependencies": "echo 'Need to build every project and reset project graph before syncing dependencies ...' && nx run-many -t build build-tsc && nx reset && nx run-many -t lint-eslint --fix --rule '@nx/dependency-checks:error'"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@commitlint/config-nx-scopes": "19.7.1",
    "@commitlint/types": "19.5.0",
    "@eslint/js": "9.20.0",
    "@nx/devkit": "20.4.6",
    "@nx/eslint": "20.4.6",
    "@nx/eslint-plugin": "20.4.6",
    "@nx/js": "20.4.6",
    "@nx/plugin": "20.4.6",
    "@nx/vite": "20.4.6",
    "@nx/workspace": "20.4.6",
    "@types/eslint": "9.6.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/mocha": "10.0.10",
    "@types/node": "22.13.5",
    "@types/yargs": "17.0.33",
    "@vitest/coverage-v8": "2.1.9",
    "@vitest/ui": "2.1.9",
    "@web/dev-server": "0.4.6",
    "@web/test-runner": "0.20.0",
    "chai": "5.2.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-n": "17.15.1",
    "globals": "15.15.0",
    "jsonc-eslint-parser": "2.4.0",
    "memfs": "4.17.0",
    "minimatch": "10.0.1",
    "mocha": "11.1.0",
    "nx": "20.4.6",
    "prettier": "3.5.1",
    "prettier-plugin-jsdoc": "1.3.2",
    "prettier-plugin-sh": "0.15.0",
    "renovate": "39.176.4",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "verdaccio": "6.0.5",
    "vite": "5.4.14",
    "vitest": "2.1.9",
    "yargs": "17.7.2"
