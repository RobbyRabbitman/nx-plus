    "@nx/devkit": "20.6.4",
    "@nx/eslint": "20.6.4",
    "@nx/eslint-plugin": "20.6.4",
    "@nx/js": "20.6.4",
    "@nx/plugin": "20.6.4",
    "@nx/vite": "20.6.4",
    "@nx/workspace": "20.6.4",
    "@types/eslint": "9.6.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/mocha": "10.0.10",
    "@types/node": "22.14.0",
    "@types/yargs": "17.0.33",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "3.1.1",
    "@web/dev-server": "0.4.6",
    "@web/test-runner": "0.20.0",
    "chai": "5.2.0",
