    "@nx/js": "20.4.6",
    "@nx/plugin": "20.4.6",
    "@nx/vite": "20.4.6",
    "@nx/workspace": "20.4.6",
    "@types/eslint": "9.6.1",
    "@types/node": "20.17.16",
    "@types/proper-lockfile": "4.1.4",
    "@types/semver": "7.5.8",
    "@types/uuid": "10.0.0",
