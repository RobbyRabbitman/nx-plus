{
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 0,
        "proseWrap": "preserve"
      }
    }
  ],
  "plugins": ["prettier-plugin-jsdoc", "prettier-plugin-sh"]
}
